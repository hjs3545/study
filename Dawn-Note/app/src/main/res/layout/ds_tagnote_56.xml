<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:background="@color/white"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <androidx.appcompat.widget.Toolbar
            android:layout_width="600dp"
            android:layout_height="75dp"
            android:background="@drawable/tagnote_titlebar_linear"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbar"
            app:layout_constraintTop_toTopOf="parent" tools:layout_editor_absoluteX="1dp"
            tools:ignore="MissingConstraints"
            app:layout_collapseMode="pin"
    />
    <Button
            android:layout_width="45dp"
            android:layout_height="45dp" android:id="@+id/ds_5_back"
            android:background="@drawable/back"
            style="?android:attr/borderlessButtonStyle"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="13dp" android:layout_marginLeft="13dp" android:layout_marginStart="13dp"/>
    <TextView
            android:text="자료구조 05,06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            android:textSize="30sp" android:textColor="@color/white"
            android:fontFamily="@font/korean"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="13dp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <FrameLayout
            android:layout_width="match_parent" android:layout_height="wrap_content"
            android:orientation="vertical" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="75dp">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">

            <TextView
                    android:text="스택 ADT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView1"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="후입선출(LIFO : last-in-first-out"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView2"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="스택(stack)이란 LIFO 접근 프로토콜을 유지하는 원소들의 컬렉션이다."
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView3"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
            />
            <TextView
                    android:text="5.2 배열 구현"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView4"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="Stack 인터페이스에 대한 연결 구현 : LinkedStack"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView5"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="큐 ADT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView6"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="선입선출(FIFO : first-in-first-out)"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView7"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="연결 구현(A Linked Implementation)"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView8"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="구현이 더 빠르다"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView9"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="공간을 낭비하지 않는다"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView10"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
            />
            <TextView
                    android:text="제거된 노드가 자동 쓰레기 수집 프로세스에 의해서 삭제되기 때문이다."
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView11"
                    android:fontFamily="@font/korean"
                    android:textSize="25sp" android:textColor="@color/black"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
            />
        </LinearLayout>
        <LinearLayout
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:orientation="vertical">
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_1"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/tag_section"
            />

            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_2"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="79dp" android:id="@+id/ds_5_tag_3"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_4"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_5"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_6"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_7"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_8"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_9"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="49dp" android:id="@+id/ds_5_tag_10"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />
            <Button
                    android:layout_width="580dp"
                    android:layout_height="79dp" android:id="@+id/ds_5_tag_11"
                    android:scaleType="fitCenter"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/tag_section"
            />

        </LinearLayout>


    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>